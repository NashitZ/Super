class Person:
    def __init__(self,id,first_name,last_name,birthday):
        self.id=id
        self.first_name=first_name
        self.last_name=last_name
        self.birthday=birthday
    
    def print_info(self):
        print("Bienvenido ")


from a import Person
class Superhero(Person):
    def __init__(self,id,first_name,last_name,birthday,alias,superpower):
        super().__init__(id,first_name,last_name,birthday)
        self.alias=alias
        self.superpower=superpower
    
    def print_info(self):
        print("Binvenido ")
       

